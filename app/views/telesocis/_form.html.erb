<%= form_for :telesoci do |f| %>

<% if @telesoci.errors.any? %>
<aside class="alert alert-danger">
  <h2>
    <%= pluralize(@telesoci.errors.count, "error") %> prohibited
    this Telesoci from being saved:
  </h2>
  <ul>
    <% @telesoci.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
    <% end %>
  </ul>
</aside>
<% end %>

<div class="form-group">
  <%= f.label :phone %><br>
  <%= f.text_field :phone, autofocus: true, class: "form-control"  %>

  <%= f.label :nickname %><br>
  <%= f.text_field :nickname, class: "form-control" %>

  <%= f.label :first_name %><br>
  <%= f.text_field :first_name, class: "form-control" %>

  <%= f.label :last_name %><br>
  <%= f.text_field :last_name, class: "form-control" %>

  <%= f.label :company_name %><br>
  <%= f.text_field :company_name, class: "form-control" %>

  <%= f.label :email %><br>
  <%= f.text_field :email, class: "form-control" %>

  <%= f.label :url %><br>
  <%= f.text_field :url, class: "form-control" %>

  <%= f.label :notes %><br>
  <%= f.text_area :notes, class: "form-control" %>
</div>

<div class="actions">
  <%= f.submit class: "btn btn-primary btn-lg" %>
</div>

<% end %>
